{
    "compilerOptions": {
        "target": "ESNext",
        "jsx": "react-jsx",
        "jsxImportSource": "hono/jsx",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "typeRoots": ["./node_modules/@types"],
        "strict": true,
        "outDir": "./dist",
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true
    },
    "tsc-alias": {
        "resolveFullPaths": true
    }
}
